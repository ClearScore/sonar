{
  "name": "@clearscore/sonar",
  "version": "2.0.1",
  "description": "Script for bumping to latest dependencies",
  "main": "src/index.js",
  "src": "src/index.js",
  "scripts": {
    "sonar": "node ./src/index.js",
    "prepublish": "yarn test",
    " --- TESTING --- ": "",
    "test": "TZ=UTC jest --config ./jest.config.js",
    "test:changed": "jest -o --watch --coverage --config ./jest.config.js",
    "lint": "eslint .",
    "lint:fix": "yarn lint --fix",
    " --- REPO UTILS --- ": "",
    "clean": "yarn clean:dist && yarn clean:yarn && yarn clean:npm",
    "clean:dist": "rimraf packages/**/dist",
    "clean:npm": "rimraf packages/**/node_modules && rimraf node_modules",
    "clean:yarn": "rimraf yarn.lock && rimraf packages/**/yarn.lock",
    "nuke": "yarn clean && yarn"
  },
  "files": [
    "/src"
  ],
  "bin": {
    "sonar": "./src/index.js"
  },
  "contributors": [
    {
      "name": "Neil Barton",
      "email": "neil.barton@clearscore.com"
    },
    {
      "name": "Peter Mouland",
      "email": "peter.mouland@clearscore.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ClearScore/sonar.git"
  },
  "keywords": [
    "sonar",
    "monorepo",
    "mono-repo",
    "dependencies",
    "tools"
  ],
  "license": "MIT",
  "dependencies": {
    "chalk": "3.0.0",
    "file-loader": "6.0.0",
    "filehound": "1.17.4",
    "find-up": "4.1.0",
    "jsonfile": "6.0.1",
    "latest-version": "5.1.0",
    "p-map": "4.0.0",
    "progress": "2.0.3",
    "semver": "7.1.3",
    "semver-diff": "3.1.1",
    "yargs": "15.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@commitlint/config-lerna-scopes": "8.3.4",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jest": "23.8.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-jsx-control-statements": "2.2.1",
    "eslint-plugin-mocha": "6.3.0",
    "eslint-plugin-postcss-modules": "1.1.2",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "2.5.0",
    "globals": "12.4.0",
    "husky": "4.2.3",
    "jest": "25.1.0",
    "jest-junit": "10.0.0",
    "lint-staged": "10.0.8",
    "prettier": "1.19.1",
    "rimraf": "3.0.2"
  }
}
